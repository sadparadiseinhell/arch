! Reread the file and apply the settings:
! xrdb -load ~/.Xresources


! Theme
!#include "PATH TO FILE"


! special
*.foreground:        #D8DEE9
*.background:        #15181e

! black
*.color0:            #3B4252
*.color8:            #4C566A

! red
*.color1:            #BF616A
*.color9:            #BF616A

! green
*.color2:            #A3BE8C
*.color10:           #A3BE8C

! yellow
*.color3:            #EBCB8B
*.color11:           #EBCB8B

! blue
*.color4:            #81A1C1
*.color12:           #81A1C1

! magenta
*.color5:            #B48EAD
*.color13:           #B48EAD

! cyan
*.color6:            #88C0D0
*.color14:           #8FBCBB

! white
*.color7:            #E5E9F0
*.color15:           #ECEFF4


! dwm normal colors
dwm.normfgcolor:     #D8DEE9
dwm.normbgcolor:     #0f1217
dwm.normbordercolor: #434C5E
dwm.normfloatcolor:  #434C5E

! dwm sel colors
dwm.selfgcolor:      #ECEFF4
dwm.selbgcolor:      #81A1C1
dwm.selbordercolor:  #81A1C1
dwm.selfloatcolor:   #B48EAD 

! dwm urgent colors
dwm.urgfgcolor:      #ECEFF4
dwm.urgbgcolor:      #BF616A
dwm.urgbordercolor:  #BF616A
dwm.urgfloatcolor:   #B48EAD


! dmenu colors
dmenu.background:    #2E3440
dmenu.foreground:    #D8DEE9
dmenu.selbackground: #81A1C1
dmenu.selforeground: #ECEFF4


! slock colors
slock.init:          #2E3440
slock.input:         #81A1C1
slock.failed:        #BF616A
slock.caps:          #EBCB8B


! Font
st.font:             DejaVu Sans Mono:pixelsize=15:antialias=true:autohint=true
dmenu.font:          Roboto:style=Bold:size=10

st.alpha: 0.98

! URxvt
URxvt.background:    [85]#15181e

*.font:          xft:DejaVu Sans Mono:pixelsize=15:antialias=true:autohint=true
URxvt.letterSpace:   0

*.internalBorder:    30

URxvt*scrollBar:     false
URxvt.scrollstyle:   plain

URxvt.saveLines:     5000
URxvt.pointerBlank:  true
URxvt.cursorBlink:   true

URxvt.geometry:      61x22+0+0

URxvt.depth:         32

*.secondaryWheel:    1

URxvt.antialias:     true

URxvt.urlLauncher:    chromium
URxvt.matcher.button: 1
URxvt.keysym.M-u:     perl:url-select:select_next
URxvt.colorUL:        #86a2be
URxvt.underlineURLs:  true
URxvt.underlineColor: yellow

!URxvt.keysym.Shift-Control-V: eval:paste_clipboard
!URxvt.keysym.Shift-Control-C: eval:selection_to_clipboard

!XTerm.termName: xterm-256color
!XTerm.vt100.scrollBar: true
!XTerm.vt100.scrollbar.width: 8
